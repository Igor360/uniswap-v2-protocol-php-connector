{
    "name": "tightenco/:package_name",
    "description": ":package_description",
    "keywords": [
        "tightenco",
        ":package_name",
        "php",
    ],
    "homepage": "https://github.com/tighten/:package_name",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": ":author_email",
            "homepage": "https://tighten.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "tightenco/tlint": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Tightenco\\:package_php_namespace\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tightenco\\:package_php_namespace\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tightenco\\:package_php_namespace\\:package_php_namespaceServiceProvider"
            ],
            "aliases": {
                ":package_php_namespace": "Tightenco\\:package_php_namespace\\:package_php_namespaceFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
